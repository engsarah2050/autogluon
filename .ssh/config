# Read more about SSH config files: https://linux.die.net/man/5/ssh_config
Host 5.tcp.eu.ngrok.io
    HostName 5.tcp.eu.ngrok.io
    UserKnownHostsFile /dev/null
    VisualHostKey yes
    Port 18003
    User root
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_rsa
    PubkeyAuthentication no
    PasswordAuthentication  yes
    
Host 2.tcp.eu.ngrok.io
    HostName 2.tcp.eu.ngrok.io
    UserKnownHostsFile /dev/null
    VisualHostKey yes
    Port 13731
    User root
    IdentitiesOnly yes
    IdentityFile ~/.ssh/id_rsa
    PubkeyAuthentication yes
    PasswordAuthentication yes

Host 4.tcp.eu.ngrok.io
    HostName 4.tcp.eu.ngrok.io
    UserKnownHostsFile /dev/null
    VisualHostKey yes
    Port 11745
    User root
    IdentitiesOnly yes
    IdentityFile ~/.ssh/authorized_keys
    PubkeyAuthentication yes

Host 7.tcp.eu.ngrok.io
    HostName 7.tcp.eu.ngrok.io
    UserKnownHostsFile /dev/null
    VisualHostKey yes
    Port 12814
    User root
    IdentitiesOnly yes
    IdentityFile ~/.ssh/authorized_keys
    PubkeyAuthentication yes

Host 6.tcp.eu.ngrok.io
    HostName 6.tcp.eu.ngrok.io
    UserKnownHostsFile /dev/null
    VisualHostKey yes
    Port 14036
    User root
    IdentitiesOnly yes
    IdentityFile ~/.ssh/authorized_keys
    PubkeyAuthentication yes


Host 0.tcp.eu.ngrok.io
    HostName 0.tcp.eu.ngrok.io
    UserKnownHostsFile /dev/null
    VisualHostKey yes
    Port 10672
    User root
    IdentityFile ~/.ssh/id_rsa_sa
    IdentitiesOnly yes

Host 0.tcp.eu.ngrok.io
    HostName 0.tcp.eu.ngrok.io
    UserKnownHostsFile /dev/null
    VisualHostKey yes
    Port 12379
    User root
    IdentityFile ~/.ssh/id_rsa_sa
    IdentitiesOnly yes # see comment in answer below



Host alias
    HostName hostname
    User user

Host google_colab_ssh
    HostName 6.tcp.ngrok.io
    User root
    Port 18817